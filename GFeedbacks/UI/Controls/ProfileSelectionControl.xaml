<UserControl x:Class="GFeedbacks.UI.ProfileSelectionControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:GFeedbacks.UI"
             mc:Ignorable="d"
             VerticalAlignment="Stretch"
             HorizontalAlignment="Stretch">
    <DockPanel VerticalAlignment="Stretch" HorizontalAlignment="Stretch">
        <Label x:Name="lblProfile" Content="Profile" VerticalAlignment="Center"/>
        <ListBox x:Name="lstbxProfile" Width="331" Margin="0,5,0,5" />
        <Button x:Name="btnAdd" Width="50" Height="20" Content="Add" Margin="5px,0,0,0" />
    </DockPanel>
</UserControl>
